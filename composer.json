{
  "name": "bo/slickcarousel",
  "description": "Slick carousel for Typo3",
  "type": "typo3-cms-extension",
  "version": "0.0.3",
  "require": {
    "typo3/cms": ">=7.6.0,<8.0",
    "fluidtypo3/vhs": "^2.4.0"
  },
  "authors": [
    {
      "name": "Boris Schauer",
      "email": "me@bschauer.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "Bo\\Slickcarousel\\": "Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web",
      "Package": {
        "partOfMinimalUsableSystem": true
      }
    }
  }
}
